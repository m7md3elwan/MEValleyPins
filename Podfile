# platform :ios, '9.0'

def testingPods
    pod 'Quick'
    pod 'Nimble'
end

target 'MEValleyLoader' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MEValleyLoader

  target 'MEValleyLoaderTests' do
    inherit! :search_paths
    testingPods
  end

end

target 'MEValleyPins' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MEValleyPins

  target 'MEValleyPinsTests' do
    inherit! :search_paths
    testingPods
  end

end
